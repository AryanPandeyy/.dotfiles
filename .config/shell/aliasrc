alias diaryy='EDITOR=vim diary ~/.diary/'
alias v='nvim'
alias dark='sudo brillo -q -U 10'
alias bright='sudo brillo -q -A 10'
alias news='newsboat'
alias am='alsamixer'
alias config='/usr/bin/git --git-dir=$HOME/.dotfiles/ --work-tree=$HOME'
alias anime='ani-cli -q 720 -e mpv'
alias hst="history 1 -1 | cut -c 8- | sort | uniq | fzf | tr -d '\n' | xclip -sel c"
